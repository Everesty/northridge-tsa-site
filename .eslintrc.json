{
  "extends": "eslint-config-airbnb",
  "settings": {
    "ecmascript": 6
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "destructuring": true,
    "classes": true,
    "forOf": true,
    "blockBindings": true,
    "arrowFunctions": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "jsx-a11y/no-static-element-interactions": "off"
  }
}
